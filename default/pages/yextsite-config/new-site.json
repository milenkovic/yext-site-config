{
  "$id": "site-${{siteName}}",
  "$schema": "https://schema.yext.com/config/pages/yextsite-config/v1",
  "siteName": "${{siteName}",
  "repoConfig": "impeding-breeze-nemesis-bb040",
  "siteSettings": {
    "autoDeploy": true,
    "autoStage": true,
    "autoPublish": false,
    "pullRequestsEnabled": false,
    "stagingCredentials": {
      "password": "",
      "username": ""
    }
  },
  "branches": [
    {
      "name": "main",
      "envVars": [
        {
          "key": "NPM_TOKEN",
          "value": "${{npmToken}}"
        }
      ],
      "isProductionBranch": true
    }
  ]
}
